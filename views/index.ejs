<!DOCTYPE html>
<html>
  <head>
    <meta charset="utf-8">
    <link rel="shortcut icon" type="image/png" href="/public/images/tab-icon.png"/>

    <title>All Users</title>
  </head>
  <body>
    <%- include('layout'); -%>
    <div class="container">
      <h1>All Users</h1>
      <div class="userKey">
        <div class="row user-box">
          <% userList.forEach(function(userData) { %>
            <div class="col-md-5 col-md-offset-2">
            <img src='/public/images/user.png'>
            <p><%= userData.firstname + ' ' + userData.lastname %></p>
            <p><%= userData.email %></p></div>
          <% }); %>
        </div>
      </div>
    </div>
  </body>
</html>
